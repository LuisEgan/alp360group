{
  "name": "miami",
  "version": "0.0.1",
  "description": "Miami",
  "dependencies": {
    "gatsby": "^2.20.2",
    "gatsby-plugin-manifest": "^2.3.1",
    "gatsby-plugin-offline": "^3.1.0",
    "gatsby-plugin-react-helmet": "^3.2.0",
    "gatsby-plugin-sass": "^2.2.0",
    "node-sass": "^4.13.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.1",
    "react-images": "^1.1.0",
    "react-scrollspy": "^3.4.2",
    "smoothscroll-polyfill": "^0.4.4"
  },
  "scripts": {
    "dev": "gatsby develop",
    "build": "npm run clean && gatsby build",
    "deploy": "npm run clean && gatsby build --prefix-paths && gh-pages -d public",
    "serve": "gatsby serve",
    "clean": "rimraf .cache public",
    "prettier": "prettier src/**/*.{ts,tsx,scss} --write",
    "lint": "eslint src/**/*.{ts,tsx} --no-error-on-unmatched-pattern",
    "lint:fix": "eslint src/**/*.{ts,tsx} --fix --no-error-on-unmatched-pattern",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@types/react-helmet": "^6.1.0",
    "@types/smoothscroll-polyfill": "^0.3.1",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.7.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "gh-pages": "^2.0.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  },
  "husky": {
    "hooks": {}
  },
  "lint-staged": {
    "*.ts": [
      "yarn run prettier --write",
      "yarn run lint:fix"
    ],
    "*.tsx": [
      "yarn run prettier --write",
      "yarn run lint:fix"
    ]
  },
  "keywords": [
    "gatsby",
    "gatsby-starter",
    "gatsby-starter-solidstate"
  ],
  "license": "MIT"
}
